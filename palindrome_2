def largest_palindrome_new():
    n1 = 1 
    n2 = 1 
    max_pal = 1
     
    for i in range(100,1000):
        for j in range(100,1000):
            num = i*j
            a = str(num)
            b = a[::-1]
            
            if a == b:
                n1,n2,max_pal = i,j,num
    return n1,n2,max_pal
    
    
    output : (995, 583, 580085)
